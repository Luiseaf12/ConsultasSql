import pandas as pd
from sqlalchemy import create_engine

# Crear engine
engine = create_engine('mssql+pyodbc://lecturaAPP:S1stem%40s@PCSISTEMASJGC22\\SQL14/FABRICA_PASSA?driver=SQL+Server')

# Consulta
query = "SELECT count(*) FROM CATCTES WHERE status = 'A'"
